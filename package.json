    "clear": "rm -rf dist docs",
    "prebuild": "yarn clear && yarn lint",
    "dev": "webpack-dev-server --hot --mode 'development'",
    "lint": "eslint 'src/**/*.js'",
    "prettier": "prettier src/**/*.js example/**/*.js --write"
