    "type": "git",
    "url": "https://github.com/mkg0/react-numeric"
  },
  "main": "dist/index.js",
  "keywords": [
    "autonumeric",
    "currency",
    "money",
    "monetary",
    "Euro",
    "Dollar",
    "Pound",
